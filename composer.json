{
  "name": "mtyrtov/langfuse-php",
  "description": "Langfuse SDK",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.9",
    "cuyz/valinor": "^1.15",
    "ramsey/uuid": "^4.7",
    "nesbot/carbon": "^2.71.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "phpstan/phpstan": "^1.12",
    "friendsofphp/php-cs-fixer": "^3.75"
  },
  "autoload": {
    "psr-4": {
      "Langfuse\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Langfuse\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "analyze": "phpstan analyze"
  },
  "minimum-stability": "stable"
}
